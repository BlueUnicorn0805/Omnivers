<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="25dip" />
            <stroke android:width="1dip" android:color="#b3c06f" />
            <gradient android:angle="-90" android:endColor="#b3c06f" android:startColor="#b3c06f" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="25dip" />
            <stroke android:width="1dip" android:color="#aacb05" />
            <gradient android:angle="-90" android:endColor="#aacb05" android:startColor="#aacb05" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="25dip" />
            <stroke android:width="1dip" android:color="#bbe002" />
            <solid android:color="#bbe002" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="25dip" />
            <stroke android:width="1dip" android:color="#bbe002" />
            <gradient android:angle="-90" android:endColor="#bbe002" android:startColor="#bbe002" />
        </shape>
    </item>
</selector>